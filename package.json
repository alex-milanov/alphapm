{
  "name": "alphapm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass",
    "build:js": "browserify --t [ babelify --presets [ es2015] ] src/js/index.js -o dist/js/app.js --debug",
    "build:sass": "node-sass --source-map dist/css/style.css.map --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "watch": "npm run watch:js & npm run watch:sass & npm run livereload",
    "watch:js": "watchify -p browserify-hmr --t [ babelify --presets [ es2015] ] src/js/index.js  -o dist/js/app.js --debug",
    "watch:sass": "nodemon -w src/sass -e sass -x 'npm run build:sass'",
    "livereload": "livereload dist/**/*.css -d",
    "start": "nodemon server & npm run watch",
    "start:proc": "npm run build && PORT=3000 nodemon server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bson-objectid": "^1.1.5",
    "deep-diff": "^0.3.8",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "iblokz-data": "^1.0.2",
    "iblokz-rest-api": "^1.1.0",
    "iblokz-snabbdom-helpers": "^1.0.0",
    "jsonwebtoken": "^7.3.0",
    "method-override": "^2.3.6",
    "moment": "^2.18.1",
    "mongoose": "^4.6.3",
    "raf-stream": "^3.0.0",
    "rx": "^4.1.0",
    "rx-node": "^1.0.2",
    "snabbdom": "^0.5.4",
    "sortablejs": "^1.5.1",
    "superagent": "^3.3.1",
    "vex-js": "^3.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^1.0.0",
    "livereload": "^0.6.0",
    "node-sass": "^3.8.0",
    "node-serve": "0.0.3",
    "nodemon": "^1.11.0",
    "watchify": "^3.8.0"
  }
}
